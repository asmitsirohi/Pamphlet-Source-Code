    <style>
        @import url('https://fonts.googleapis.com/css2?family=Ubuntu&display=swap');

        body {
            font-family: 'Ubuntu', sans-serif;
        }


        * {
            margin: 0;
            padding: 0;
        }

        .fab {
            font-size: 28px;
            padding: 10px 12px;

        }

        .navbar.bg-dark {
            background-color: #060606 !important;
            border: none;
            padding: 25px;
        }

        .nav-item a {
            margin-left: 25px !important;
        }

        .home {
            margin-top: 120px !important;
            margin-bottom: 170px !important;
        }


        .row h2 span {
            color: red;
        }

        .skills {
            margin-top: 141px !important;
            margin-bottom: 170px !important;
        }

        .skill-content {
            margin-top: 4.5rem !important;
        }

        .container h4 {
            margin-top: 80px;
        }

        .ex-content {
            margin-top: 53px;
        }

        .project-content {
            margin-top: 130px !important;
            margin-bottom: 120px;
        }

        .container .card {
            box-shadow: 20px 20px 50px rgba(0, 0, 0, 0.5);
            border-radius: 15px;
            background: rgba(255, 255, 255, 0.1);
            overflow: hidden;
            display: flex;
            justify-content: center;
            align-items: center;
            border-top: 1px solid rgba(255, 255, 255, 0.5);
            border-left: 1px solid rgba(255, 255, 255, 0.5);
            backdrop-filter: blur(5px);
        }

        .container .card .content {
            padding: 20px;
            text-align: center;
            transition: 0.5s;
        }

        .container .card .content h3 {
            position: absolute;
            top: -7px;
            right: 30px;
            font-size: 5em;
            color: rgba(255, 255, 255, 0.5);
            pointer-events: none;
        }

        .container .card .content p {
            margin-top: 55px;
            font-size: 1em;
            font-weight: 300;
            color: white;
        }

        .mb-4 a {
            position: relative;
            display: inline-block;
            padding: 8px 20px !important;
            margin-top: 10px !important;
            background: white !important;
            color: #060606 !important;
            border-radius: 20px !important;
            text-decoration: none !important;
            font-weight: 500 !important;
        }


        .about-content {
            margin-top: 50px !important;

        }



        #contactus {
            margin-top: 120px !important;
            margin-bottom: 80px !important;
        }

        .footer {
            margin-bottom: 10px;
        }

        .footer h4 {
            font-weight: bold;
            color: white;
            padding: 20px 0;
        }

        .footer input {
            width: 60%;
            background-color: black;
            outline: none;
            border: 0;
            border-bottom: 1px solid white;
            padding: 10px;
            margin-bottom: 30px;
            text-align: center;
            color: azure;
        }

        .footer textarea {
            width: 60%;
            margin-left: 20%;
            background-color: black;
            outline: none;
            border: 0;
            border-bottom: 1px solid white;
            padding: 10px;
            margin-bottom: 30px;
            text-align: center;
            color: azure;
        }

        ::placeholder {
            color: azure;
            font-size: 15px;
        }

        .footer button {
            padding: 7px;
            width: 180px;
            margin: auto;
            background-color: transparent;
            border: 1px solid white;
            font-size: 20px;
            display: block;
            color: aliceblue;
        }

        .footer button:hover {
            background-color: aliceblue;
            color: black;
            transition: 0.7s;
        }

        .alert {
            position: absolute;
            z-index: 1000;
            width: 99.5vw;
            display: none;
            margin-top: 123px;
            margin-left: -23px;
        }

        .alert strong {
            margin: 0;
        }

        .active-box {
            background-color: rgb(118, 37, 103);
            color: aliceblue;
        }
    </style>

    {{#each usertemplate }}
    <!-- ******************************* NAVBAR-SECTION STARTED *****************************-->

    <div class="container-fluid">
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
            <a class="navbar-brand" href="#">Portfolio</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#skill">Skill</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#projects">Projects</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#about">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#contactus">Contact Us</a>
                    </li>
                </ul>
            </div>
            <div class="alert alert-success alert-dismissible fade show" role="alert" id="mailsent" style="display: none;">
                <strong>Success! </strong>Your Details have been recieved.
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
    </div>
    </nav>
    </div>

    <!-- ********************************* HOME-SECTION STARTED **********************************-->

    <div class="container">
        <section class="home">
            <div class="row">
                <div class="col-sm-6 mt-5">
                    <h1 class="font-weight-bolder">
                        Hello, I am {{name}}.
                    </h1><br>
                    <h2>
                        <script>
                            let obj = [];
                            `{{#each key_points}}
                            ${obj.push(`{{this}}`)}
                            {{/each}}`;

                            document.write(`
                                I Am a <span class="typewrite element" data-period="2000"
                                    data-type='${JSON.stringify(obj)}'>
                                    <span class="wrap"></span>
                                </span>
                            `);
                        </script>
                    </h2>
                </div>


                <div class="col-sm-6">
                    <img src="{{profileImage}}" alt="Profile Pic"
                        class="profileImage img-fluid rounded mx-auto d-block" width="250px">

                    <p class="text-center links">
                        {{#each links as |link|}}
                            {{#if twitter}}
                            <a href="{{twitter}}"><i class="fab fa-twitter"></i></a>
                            {{else}}
                            {{#if linkedin}}
                                <a href="{{linkedin}}"><i class="fab fa-linkedin"></i></a>
                            {{else}}
                                {{#if facebook}}
                                <a href="{{facebook}}"><i class="fab fa-facebook-f"></i></a>
                                {{else}}
                                {{#if instagram}}
                                    <a href="{{instagram}}"><i
                                        class="fab fa-instagram"
                                    ></i></a>
                                {{else}}
                                    <a href="{{github}}"><i class="fab fa-github"></i></a>
                                {{/if}}
                                {{/if}}
                            {{/if}}
                            {{/if}}
                        {{/each}}
                    </p>
                </div>
            </div>
        </section>

        <!--*************************************SKILL-SECTION STARTED  *********************************-->

        <section class="skills text-center" id="skill">
            <h2>Skills</h2>
            <div class="row my-4 skill-content">
                 {{#each skills}}
                <div class="col-sm-4 my-4">
                    <div class="progress" style="height: 20px;">
                        <div class="progress-bar bg-success progress-bar-striped progress-bar-animated"
                            role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100"
                            style="width: 60%">{{this}}</div>
                    </div>
                </div>
                {{/each}}
            </div>
        </section>

        <!-- ****************************PROJECT-SECTION STARTED**************************************      -->

        <section class="projects text-center" id="projects">
            <h1>Projects</h1>

            <div class="row project-content">
                {{#each projects}}
                <div class="col-sm-4 mb-4">
                    <div class="card">
                        <div class="card-body mb-4">
                            <h3>{{projectName}}</h3>
                            <p class="my-4">{{{projectDescription}}}</p>
                            <a href="{{projectLink}}" class="mb-4">View Project</a>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </section>

        <sesction class="about" id="about">
            <div class="container">
                <h1 class="text-center">ABOUT</h1>
                <div class=" about-content">
                    <p>{{{about}}}</p>
                </div>
        </sesction>
        </sesction>

        <!--******************************** CONTACTUS SECTION STARTED ********************************-->

        <section class="footer text-center" id="contactus">
            <h4>
                Let's get in touch. Send me a message
            </h4>
            <form id="userEmailForm" class="mb-4">
                <input type="text " placeholder="Name " id="username" required>
                <input type="email " placeholder=" Enter Your Email " id="useremail" required>
                <input type="text " placeholder=" Subject " id="subject" required>
                <input type="hidden" id="hostemail" value="{{email}}" required>
                <input type="hidden" id="hostname" value="{{name}}" required>
                <div class="form-group">
                    <!-- <label for="exampleFormControlTextarea1">Message</label> -->
                    <textarea class="form-control" id="message" rows="3"
                        placeholder="Type Your Message Here..." id="message" required></textarea>
                </div>

                <div id="nonSpinnerBtn">
                    <button type="submit" >Submit</button>
                </div>
                <div id="spinnerBtn" style="display: none;">
                    <button id="spinnerBtn" type="button" disabled>
                        <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                        Sending...
                    </button>
                </div>
            </form>
        </section>
    </div>

    <script>
      document.title = 'Portfolio-{{name}}';
    </script>
    {{/each}}

